                    {block:IfHeaderImage}<a href="/"><img src="{image:Header}" alt="{Title}" /></a>{/block:IfHeaderImage}
                    {block:IfNotHeaderImage}<a href="/">{Title}</a>{/block:IfNotHeaderImage}
                </h1>
            </div><!-- #branding -->
                <div id="about" class="widget">
                    <h3>About</h3>
                    <a href="http://www.tumblr.com/follow/{Name}"><img src="{PortraitURL-64}" alt="portrait" /></a>
                    {block:Description}<div class="description"><p>{Description}</p></div>{/block:Description}
                    <p>I'm a self proclaimed artist. Visit my <a href="http://dzv13.deviantart.com">dA</a>. Click To Reload Page With Some <a href="#Music" onclick="window.location.reload();">Nice Blog Music</a>. <br> <a href="/CoolQuotes">Click here for deep quotes</a> Or even better <a href="/tagsUsed">Click here</a> to see all my <a href="/tagsUsed">Tags</a></p>
                </div>   
        </div><!-- #header -->
        <div id="main">
            <div id="container">
                <div id="content">
                {block:Posts}
                    <div id="post-{PostID}" class="post {block:Text}post-type-text{/block:Text}{block:Photo}post-type-photo{/block:Photo}{block:Photoset}post-type-photoset{/block:Photoset}{block:Quote}post-type-quote{/block:Quote}{block:Link}post-type-link{/block:Link}{block:Chat}post-type-chat{/block:Chat}{block:Video}post-type-video{/block:Video}{block:Audio}post-type-audio{/block:Audio}">
                            <div class="post-type">
                                <a href="{Permalink}">{block:Text}Text{/block:Text}{block:Photo}Photo{/block:Photo}{block:Photoset}Photoset{/block:Photoset}{block:Quote}Quote{/block:Quote}{block:Link}Link{/block:Link}{block:Chat}Chat{/block:Chat}{block:Video}Video{/block:Video}{block:Audio}Audio{/block:Audio}</a>
                            </div><!-- .type -->
                        <div class="post-content">
                        {block:Text}
                            {block:Title}<h3><a href="{Permalink}">{Title}</a></h3>{/block:Title}
                            {Body}
                        {/block:Text}
                        {block:Photo}
                            {LinkOpenTag}<img src="{PhotoURL-500}" class="photo" alt="{PhotoAlt}" />{LinkCloseTag}
                            {block:HighRes}<a href="{PhotoURL-HighRes}" class="high-res" target="_blank"></a>{/block:HighRes}
                            {block:Caption}<div class="caption">{Caption}</div>{/block:Caption}
                        {/block:Photo}
                        {block:Photoset}
                            {Photoset-500}
                            {block:Caption}<div class="caption">{Caption}</div>{/block:Caption}
                        {/block:Photoset}
                        {block:Quote}
                            <div class="quote-content">"{Quote}"</div>
                            {block:Source}<p class="quote-source">&mdash; {Source}</p>{/block:Source}
                        {/block:Quote}
                        {block:Link}
                            <div class="link-wrap"><h3><a href="{URL}" {Target}>{Name}</a></h3></div>
                            {block:Description}<div class="description">{Description}</div>{/block:Description}
                        {/block:Link}
                        {block:Chat}
                            {block:Title}<h3><a href="{Permalink}">{Title}</a></h3>{/block:Title}
                            <ul class="chat-wrap">
                            {block:Lines}
                                <li class="{Alt} user_{UserNumber}">
                                    {block:Label}<span class="label">{Label}</span>{/block:Label}
                                    {Line}
                                </li>
                            {/block:Lines}
                            </ul>
                        {/block:Chat}
                        {block:Audio}
                            <div class="audio-wrap">
                                <div class="audio-player">{AudioPlayer}</div>
                                {block:TrackName}{TrackName}{/block:TrackName}
                                {block:Artist}<br>By: {Artist}{/block:Artist}
                                {block:Album}<br>In: {Album}{/block:Album}
                                {block:PlayCount}<br>With: {PlayCountWithLabel} and counting{/block:PlayCount}
                                {block:AlbumArt}<img src="{AlbumArtURL}" style="height: 100px;position: absolute; right: 0px; top: 10px;" />{/block:AlbumArt}
                            </div>
                            {block:Caption}<div class="caption">{Caption}</div>{/block:Caption}
                        {/block:Audio}
                        
                        
                        {block:Video}
                            <div class="video-wrap">{Video-500}</div>
                            {block:Caption}<div class="caption">{Caption}</div>{/block:Caption}
                        {/block:Video}
                        
                        {block:HasTags}
                            <div class="tags">
                                Tags: {block:Tags}<a href="{TagURL}" class="single-tag">{Tag}</a> {/block:Tags}
                            </div>
                        {/block:HasTags}
                        </div><!-- post-content -->
                        <div class="post-meta">
                        {block:Date}
                            <div class="date">
                                <a href="{Permalink}">{Month} {DayOfMonthWithZero}, {Year}</a>
                            </div><!-- .date -->
                        {/block:Date}
                        {block:NoteCount}
                            <div class="note-count">
                                <a href="{Permalink}#notes">{NoteCountWithLabel}</a>
                            </div><!-- .note-count -->
                        {/block:NoteCount}
                        </div><!-- .post-meta -->
                        {block:IfShowShareThisPostLinks}{block:PermalinkPage}
                        <div class="share-this-post">
                            <div class="facebook-button"><fb:like layout="standard" show_faces="false" width="450" action="like" colorscheme="light"></fb:like></div>
                            <div class="tweetmeme-button"><script type="text/javascript">tweetmeme_style = 'compact';</script><script type="text/javascript" src="http://tweetmeme.com/i/scripts/button.js"></script></div>
                        </div>
                        {/block:PermalinkPage}{/block:IfShowShareThisPostLinks}
                    </div><!-- .post -->
                {block:PermalinkPage}
                    <div class="post-footer">
                        <div class="author">Posted by <a href="{PostAuthorURL}">{PostAuthorName}</a></div>
                        <div class="shorturl"><a href="{ShortURL}">&infin; Short URL</a></div>
                    </div>
                {/block:PermalinkPage}
                    {block:PostNotes}<div id="post-notes">{PostNotes}</div>{/block:PostNotes}
                {/block:Posts}
                {block:Pagination}
                    <div class="pagination">
                        <span class="archive"><a href="/archive">Archive</a></span>
                        <span class="random"><a href="/random">Random</a></span>
                        {block:NextPage}<span class="next-page"><a href="{NextPage}">Next Page</a></span>{/block:NextPage}
                        <span class="page-number">Page {CurrentPage} of <a id="urlChange">{TotalPages}</a></span>
                        {block:PreviousPage}<span class="previous-page"><a href="{PreviousPage}">Previous Page</a></span>{/block:PreviousPage}
                    </div><!-- .pagination -->
                    <script> 
                    document.getElementById('urlChange').href = window.location.href.split('/page/')[0]+"/page/{TotalPages}";
                    document.getElementById('urlChange').href = document.getElementById('urlChange').href.replace('//page','/page');
                    </script>
                {/block:Pagination}
                </div><!-- #content -->
            </div><!-- #container -->
            {block:IndexPage}
            <div class="sidebar">
            {block:IfShowPeopleIFollowWidget}{block:Following}
                <div id="people-i-follow" class="widget">
                    <h3>People I follow (In Most-Recently-Updated Order)</h3>
                    <ul>
                        {block:Followed}
                            <li><a href="{FollowedURL}"><img src="{FollowedPortraitURL-30}" alt="{FollowedName}"/></a></li>
                        {/block:Followed}
                    </ul>
                </div>
            {/block:Following}{/block:IfShowPeopleIFollowWidget}
                <div id="stuff-i-like" class="widget carousel">
                    <h3><a href="http://www.tumblr.com/liked/by/{Name}">All Stuff I Like</a></h3>
                </div>
            
            </div><!-- .sidebar -->
            {/block:IndexPage}
        </div><!-- #main -->
        <div id="footer">
            <div class="copyright" style="float: left">&copy; <a href="/">{Title}</a>. All Rights Reserved.</div>
            <div class="colophon" style="float: right">Powered by <a href="http://tumblr.com">Tumblr</a>. <a href="http://www.tumblr.com/theme/10820">Lightweight Theme</a> by <a href="http://arturkim.com">Artur Kim</a></div>
        </div><!-- #footer -->
    </div><!-- #wrapper -->
<script>
/*J/K Scroll and Left/Right Pages*/ 
document.body.onkeyup = function (e) {
if (!this.style.overflow) {
var item; 
switch (e.which || e.keyCode) {
case 74: 
	item = $('.post').filter(function (i, item) { return item.offsetTop > document.body.scrollTop; }).first()[0]; 
	if (item !== undefined) { 
		$(document.body).animate({scrollTop: item.offsetTop}, 150); 
	} else {
		$(document.body).animate({scrollTop: document.body.offsetHeight}, 150); 
	} break; /*J Down*/ 
case 75: 
	item = $('.post').filter(function (i, item) { return item.offsetTop < document.body.scrollTop; }).last()[0]; 
	if (item !== undefined) { 
		$(document.body).animate({scrollTop: item.offsetTop}, 150);
	} else {
		$(document.body).animate({scrollTop: document.body.offsetTop}, 150); 
	} break; /*K Up*/ 
case 39: //Right
	item = $('.next-page a')[0]; 
	if (item !== undefined) { location.replace(item.href); } 
	break;
case 37: //Left
   item = $('.previous-page a')[0]; 
   if (item !== undefined) { location.replace(item.href); }
   break;
} } }
window.onload = function() {
    var all = document.getElementsByClassName('tc-link');
    for (var i=0; i<all.length; i++) {
        all[i].href = all[i].href.replace(/%20/g,'+');
    }
}
</script>
</body>
</html>